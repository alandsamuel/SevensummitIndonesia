<?xml version="1.0" encoding="utf-8"?>
<android.support.v7.widget.LinearLayoutCompat xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.aradea.informasi.sevensummitindonesia.jalursemeru">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@android:color/holo_green_light"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="8dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:weightSum="1">

            <TextView
                android:id="@+id/textView8"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="2.83"
                android:text="Jalur Pendakian"
                android:textAlignment="center"
                android:textColor="@color/colorPrimaryDark"
                android:textSize="30sp"
                android:textStyle="bold" />

            <Space
                android:layout_width="match_parent"
                android:layout_height="27dp" />

            <TextView
                android:id="@+id/textView12"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="2.83"
                android:text="@string/jalursemeru"
                android:textSize="12sp" />

            <Space
                android:layout_width="match_parent"
                android:layout_height="27dp"
                android:layout_weight="2.83" />

            <TextView
                android:id="@+id/textView14"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="2.83"
                android:text="Etape Desa Ranu Pani – Danau Ranu Kumbolo"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/textView15"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="2.83"
                android:text="@string/jalursemeru1"
                android:textSize="12sp" />

            <Space
                android:layout_width="match_parent"
                android:layout_height="27dp"
                android:layout_weight="2.83" />

            <TextView
                android:id="@+id/textView16"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="2.83"
                android:text="Etape Ranu Kumbolo – Kali Mati."
                android:textStyle="bold" />

            <TextView
                android:id="@+id/textView17"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="2.83"
                android:text="@string/jalursemeru2"
                android:textAlignment="textStart"
                android:textSize="12sp" />

            <Space
                android:layout_width="match_parent"
                android:layout_height="27dp"
                android:layout_weight="2.83" />

            <TextView
                android:id="@+id/textView18"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="2.83"
                android:text="Etape Kali Mati – Arcopodo"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/textView19"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="2.83"
                android:text="@string/jalursemeru3"
                android:textAlignment="textStart"
                android:textSize="12sp" />

            <Space
                android:layout_width="match_parent"
                android:layout_height="27dp"
                android:layout_weight="2.83" />

            <TextView
                android:id="@+id/textView20"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="2.83"
                android:text="Etape Arcopodo – Puncak Mahameru."
                android:textAlignment="textStart"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/textView21"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="2.83"
                android:text="@string/jalursemeru4"
                android:textAlignment="textStart"
                android:textSize="12sp" />

        </LinearLayout>
    </ScrollView>

</android.support.v7.widget.LinearLayoutCompat>
